{
  "name": "UniPowerReactAssignment",
  "version": "1.0.0",
  "description": "UniPowerReactTest",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --progress --colors --config ./webpack.config.js --host 0.0.0.0",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "check": "npm run test"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>src/testSetup/setupTests.js",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/testSetup/testMockData/styleMock.js",
      "^components(.*)$": "<rootDir>/src/components$1",
      "^testSetup(.*)$": "<rootDir>/src/testSetup$1",
      "^src(.*)$": "<rootDir>/src$1"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --semi=false --jsx-bracket-same-line --print-width 140 --write",
      "npm run lint"
    ]
  },
  "license": "UNLICENSED",
